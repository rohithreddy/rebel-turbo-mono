    "build:prod": "pnpm with-env:prod next build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "pnpm with-env:dev next dev --turbopack",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env:dev next start",
    "start:dev": "pnpm with-env:dev next start",
    "start:prod": "pnpm with-env:prod next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "with-env:dev": "dotenv -e ../../.env.development --",
    "with-env:prod": "dotenv -e ../../.env.prod --"
  },
